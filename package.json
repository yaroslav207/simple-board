{
  "name": "simple-board",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:ts": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s . -o build",
    "start:dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/server.ts",
    "start": "ts-node --files -r tsconfig-paths/register ./src/server.ts",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "pre-start": "npm run migrate:up && npm run seed:up && npm run start:dev"
  },
  "author": "sprynsian.yaroslav.cr@gmail.com",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.10",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "tslib": "^2.4.1"
  }
}
